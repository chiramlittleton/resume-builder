\documentclass{article}
\usepackage[margin=1in]{geometry}
\usepackage[utf8]{inputenc}

\begin{document}

\section*{Name}
{{ name }}

\section*{Contact}
{{ contact.email }} \\
{{ contact.phone }} \\
{{ contact.website }}

\section*{Skills}
\begin{itemize}
{% for skill in skills %}
  \item {{ skill }}
{% endfor %}
\end{itemize}

\section*{Certificates}
\begin{itemize}
{% for cert in certificates %}
  \item {{ cert }}
{% endfor %}
\end{itemize}

\section*{Education}
{% for edu in education %}
\textbf{ {{ edu.degree }} }, {{ edu.school }} \hfill {{ edu.years }} \\
{% endfor %}

\section*{Experience}
{% for job in experience %}
\textbf{ {{ job.title }} } at {{ job.company }} \hfill {{ job.date }} \\
\begin{itemize}
{% for bullet in job.bullets %}
  \item {{ bullet }}
{% endfor %}
\end{itemize}
\vspace{0.5em}
{% endfor %}

\section*{Projects}
{% for project in projects %}
\textbf{ {{ project.name }} } \hfill {{ project.date }} \\
\begin{itemize}
{% for bullet in project.bullets %}
  \item {{ bullet }}
{% endfor %}
\end{itemize}
\vspace{0.5em}
{% endfor %}

\end{document}
